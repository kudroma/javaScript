&nbsp;&nbsp;&nbsp;&nbsp;this.firstName&nbsp;= first;<br>
&nbsp;&nbsp;&nbsp;&nbsp;this.lastName&nbsp;= last;<br>
&nbsp;&nbsp;&nbsp;&nbsp;this.age&nbsp;= age;<br>
&nbsp;&nbsp;&nbsp;&nbsp;this.eyeColor&nbsp;= eye;<br>
}<br>
var&nbsp;myFather =&nbsp;new&nbsp;person(&quot;John&quot;,&nbsp;&quot;Doe&quot;,&nbsp;50,&nbsp;&quot;blue&quot;);<br>
var&nbsp;myMother =&nbsp;new&nbsp;person(&quot;Sally&quot;,&nbsp;&quot;Rally&quot;,&nbsp;48,&nbsp;&quot;green&quot;);<o:p></o:p></span></p>

<p class=MsoNormal><span lang=EN-US style='color:#595959;mso-themecolor:text2;
